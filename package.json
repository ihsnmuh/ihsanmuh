{
  "name": "ihsanmuh",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/generate-rss.mjs && next-sitemap",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "eslint src/",
    "lint:fix": "eslint src --fix && yarn format",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "generate": "prisma generate",
    "prepare": "husky install",
    "docker:build": "docker build -f docker/Dockerfile -t ihsanmuh:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env ihsanmuh:latest"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-query-devtools": "^4.35.0",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.4",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "motion": "^11.11.17",
    "next": "^16.1.6",
    "next-nprogress-bar": "^2.3.15",
    "next-themes": "^0.2.1",
    "postcss": "8.4.26",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^4.12.0",
    "react-lite-youtube-embed": "^2.4.0",
    "reading-time-estimator": "^1.11.0",
    "rehype-pretty-code": "^0.13.2",
    "rss": "^1.2.2",
    "sharp": "^0.33.5",
    "shiki": "^1.6.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@next/bundle-analyzer": "^16.1.6",
    "@prisma/client": "^5.7.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.12.0",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/rss": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "jsdom": "^28.0.0",
    "lint-staged": "^14.0.1",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-code-titles": "^0.1.2",
    "remark-gfm": "^4.0.0",
    "ts-node": "^10.9.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "yarn prettier --write"
    ]
  },
  "prisma": {
    "seed": "yarn ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
